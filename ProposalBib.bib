journal = {Science},
keywords = {Genes,Genes: radiation effects,Humans,Legislation as Topic,Lung Neoplasms,Lung Neoplasms: etiology,Neoplasms,Nuclear Reactors,Nuclear Reactors: standards,Occupational Medicine,Plutonium,Plutonium: adverse effects,Power Plants,Power Plants: standards,Radiation-Induced,Radiation-Induced: epidemiology,Radioactive Waste,United States},
month = apr,
number = {4285},
pages = {25--34},
pmid = {841337},
title = {{The distrust of nuclear power}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/841337 http://www.sciencemag.org/cgi/doi/10.1126/science.841337},
volume = {196},
year = {1977}
}
@article{He2013,
abstract = {Concentrations and species of iodine isotopes ((127)I and (129)I) provide vital information about iodine geochemistry, environmental conditions and water masses exchange in oceans. Despite extensive investigations of anthropogenic (129)I in the Arctic Ocean and the Nordic Seas, concentrations of the isotope in the Atlantic Ocean are, however, still unknown. We here present first data on (129)I and (127)I, and their species (iodide and iodate) in surface water transect along the northeastern Atlantic between 30° and 50°N. The results show iodate as the predominant species in the analyzed marine waters for both (127)I and (129)I. Despite the rather constant ratios of (127)I(-)/(127)IO3(-), the (129)I(-)/(129)IO3(-) values reveal variations that apparently response to sources, environmental conditions and residence time. These findings provide a new tracer approach that will strongly enhance the application of anthropogenic (129)I in ocean environments and impact on climate at the ocean boundary layer.},
author = {He, Peng and Hou, Xiaolin and Aldahan, Ala and Possnert, G\"{o}ran and Yi, Peng},
doi = {10.1038/srep02685},
file = {:C$\backslash$:/Users/machen/Documents/Mendeley Papers/He et al.\_2013\_Iodine isotopes species fingerprinting environmental conditions in surface water along the northeastern Atlantic Ocean.pdf:pdf},
issn = {2045-2322},
journal = {Scientific reports},
month = jan,
pages = {2685},
pmid = {24284916},
title = {{Iodine isotopes species fingerprinting environmental conditions in surface water along the northeastern Atlantic Ocean.}},
