@article{Hou2009,
abstract = {As a long-lived beta-emitting radioisotope of iodine, (129)I is produced both naturally and as a result of human nuclear activities. At present time, the main part of (129)I in the environment originates from the human nuclear activity, especially the releases from the spent nuclear fuel reprocessing plants, the (129)I/(127)I ratios have being reached to values of 10(-10) to 10(-4) in the environment from 10(-12) in the pre-nuclear era. In this article, we review the occurrence, sources, inventory, and concentration level of (129)I in environment and the method for speciation analysis of (129)I in the environment. Measurement techniques for the determination of (129)I are presented and compared. An overview of applications of (129)I speciation in various scientific disciplines such as radiation protection, waste depository, and environmental sciences is given. In addition, the bioavailability and radiation toxicity (dose to thyroid) of (129)I are discussed.},
author = {Hou, Xiaolin and Hansen, Violeta and Aldahan, Ala and Possnert, G\"{o}ran and Lind, Ole Christian and Lujaniene, Galina},
doi = {10.1016/j.aca.2008.11.013},
file = {:C$\backslash$:/Users/machen/Documents/Mendeley Papers/Hou et al.\_2009\_A review on speciation of iodine-129 in the environmental and biological samples.pdf:pdf},
issn = {1873-4324},
journal = {Analytica chimica acta},
keywords = {Animals,Atmosphere,Atmosphere: chemistry,Biological Availability,Environment,Geologic Sediments,Geologic Sediments: chemistry,Humans,Iodine Radioisotopes,Iodine Radioisotopes: analysis,Iodine Radioisotopes: chemistry,Radioactive Waste,Soil,Soil: analysis,Water,Water: chemistry},
month = jan,
number = {2},
pages = {181--96},
pmid = {19110092},
title = {{A review on speciation of iodine-129 in the environmental and biological samples.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19110092},
volume = {632},
year = {2009}
}
@article{Hohenemser1977,
abstract = {Society seems content to strike a more moderate or uncertain balance with other technologies than with nuclear power. This attitude is traced to the social history of nuclear power, the genuine uncertainty and complexity of safety issues, underestimation of the regulatory task, and the rancorous nature of the debate. Nuclear power is not just another problem of technology, of environment, or of health. It is unique in our time. To be more demanding of nuclear safety may be to apply a double standard, but not necessarily an irrational one. Our best course appears to be to keep the nuclear option open, work toward the rapid resolution of problems such as waste disposal, but postpone recycling and the breeder reactor. Time is needed to resolve immediate problems such as transport and disposal of nuclear wastes; to come to terms with trans-scientific issues such as plutonium toxicity, sabotage, and weapons proliferation; and to evaluate long-term energy alternatives.},
author = {Hohenemser, C and Kasperson, R and Kates, R},
doi = {10.1126/science.841337},
file = {:C$\backslash$:/Users/machen/Documents/Mendeley Papers/Hohenemser, Kasperson, Kates\_1977\_The distrust of nuclear power.pdf:pdf},
